#!/bin/bash

loggedInUser=`/bin/ls -l /dev/console | /usr/bin/awk '{ print $3 }'`
SBUfolder="/Users/$loggedInUser/Library/Containers/io.fti.SplashBuddy/"


if [ -d "$SBUfolder" ]
then
		touch /Library/ClientEngineering/DEP/.InitialConfigurationComplete
		jamf recon
		sudo jamf flushPolicyHistory
		sleep 3
		jamf policy -trigger DEPReSetup1.8
fi



